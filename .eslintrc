{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "rules": {
    "no-console": 0,
    "prettier/prettier": 2,
    "indent": ["error", 4],
    "@typescript-eslint/no-unused-vars": ["warn", { "argsIgnorePattern": "next" }],
    "@typescript-eslint/no-explicit-any": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}
